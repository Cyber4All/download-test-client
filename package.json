{
  "name": "downloads-test-client",
  "version": "1.2.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "start": "npm run serve",
    "test": "node_modules/.bin/jest",
    "newman-test": "newman run System\\ Status\\ Lambda\\ Express\\ API.postman_collection.json",
    "start-server": "nodemon",
    "serve": "node -r ts-node/register ./src/adapters/app.ts",
    "serve:debug": "node --inspect=5858 -r ts-node/register ./src/app.ts",
    "build": "tsc",
    "deploy": "serverless deploy",
    "ci": "start-server-and-test start-server http://localhost:4800 newman-test"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "source-map-support": "^0.5.10",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/jest": "^26.0.21",
    "@types/mongodb": "^3.3.9",
    "@types/node": "^10.12.18",
    "jest": "^26.6.3",
    "serverless-webpack": "^5.3.1",
    "start-server-and-test": "^1.10.9",
    "ts-jest": "^26.5.4",
    "ts-loader": "^5.3.3",
    "tslint": "^5.20.1",
    "typescript": "^4.1.5",
    "webpack": "^4.41.6",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
